{
  "name": "wickdninja",
  "version": "1.0.1",
  "description": "Personal npm card for Nate Ross (@wickdninja)",
  "main": "dist/card.js",
  "unpkg": "index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/wickdninja/wickdninja.git"
  },
  "scripts": {
    "prepare": "node src/htmlify",
    "prepublishOnly": "rm -rf dist && webpack"
  },
  "bin": {
    "wickdninja": "bin/index.js"
  },
  "files": [
    "bin",
    "dist",
    "index.html"
  ],
  "keywords": [
    "card",
    "npm",
    "npm card",
    "npx",
    "npx card",
    "business card"
  ],
  "author": "Nate Ross",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "ansi-to-html": "^0.6.9",
    "babel-loader": "^8.0.4",
    "boxen": "^2.1.0",
    "chalk": "^2.4.1",
    "chalker": "^1.0.0",
    "lodash.get": "^4.4.2",
    "webpack": "^4.28.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2"
  },
  "myCard": {
    "info": {
      "name": "Nate Ross",
      "handle": "wickdninja",
      "twitter": "wickdninja",
      "npm": "wickdninja",
      "github": "wickdninja",
      "linkedin": "nate-ross-1593a477",
      "web": "https://wickd.ninja",
      "work": "Senior Developer <red>@AlliedPayment</>"
    },
    "data": [
      "          <orange>{{name}}</><white> / </><green>{{handle}}</>",
      "",
      {
        "label": "<white>Work</>",
        "text": "{{work}}"
      },
      "",
      {
        "label": "<yellow>Twitter</>",
        "text": "https://twitter.com/<green>{{twitter}}</>",
        "when": "{{twitter}}"
      },
      {
        "label": "<yellow>npm</>",
        "text": "https://www.npmjs.com/<green>~{{npm}}</>",
        "when": "{{npm}}"
      },
      {
        "label": "<yellow>GitHub</>",
        "text": "https://github.com/<green>{{github}}</>",
        "when": "{{github}}"
      },
      {
        "label": "<yellow>LinkedIn</>",
        "text": "https://www.linkedin.com/in/<green>{{linkedin}}</>",
        "when": "{{linkedin}}"
      },
      {
        "label": "<yellow>Web</>",
        "text": "<cyan>{{web}}</>",
        "when": "{{web}}"
      },
      "",
      {
        "label": "<white>Card</>",
        "text": "<white>npx</> <green>{{_packageName}}</>"
      }
    ]
  }
}
